<Window x:Class="magic.characterLoad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:magic"
        mc:Ignorable="d"
        Title="characterLoad" Height="450" Width="800">
    <Grid>
        <Button x:Name="Load" VerticalAlignment="Bottom" Content="Load" Height="36" Margin="32,0,624,62" Click="Load_Click"/>
        <Button x:Name="Delete" VerticalAlignment="Bottom" Content="Delete" Height="36" Margin="611,0,45,62" Click="Delete_Click"/>
        <Button x:Name="Back" VerticalAlignment="Top" Content="Back" Height="36" Margin="328,336,328,0" Click="backToMain"/>
        <ListBox Name="charList" Margin="90,22,90,124"/>

    </Grid>
</Window>
